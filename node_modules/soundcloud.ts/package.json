{
    "name": "soundcloud.ts",
    "version": "0.5.0",
    "description": "Soundcloud API v2 wrapper with typings. ",
    "main": "dist/soundcloud.js",
    "types": "dist/soundcloud.d.ts",
    "scripts": {
        "start": "tsc && node dist/start.js",
        "docs": "typedoc",
        "build": "tsc",
        "build:check": "tsc --noEmit",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "test": "ts-mocha --timeout 30000 -p tsconfig.json test/**/*.spec.ts",
        "test:coverage": "nyc npm run test",
        "prettier": "prettier --write \"**/*.{ts,json,yml,yaml,md}\"",
        "prepack": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Tenpi/soundcloud.ts"
    },
    "keywords": [
        "soundcloud",
        "typescript",
        "ts",
        "audio",
        "music",
        "api",
        "api-v2",
        "v2",
        "download",
        "downloader"
    ],
    "author": "Tenpi (https://github.com/Tenpi)",
    "contributors": [
        "Skick (https://github.com/skick1234)"
    ],
    "license": "MIT",
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@tenpi/tslint": "^1.1.0",
        "@tenpi/typedoc": "0.0.3",
        "@types/chai": "^4.3.4",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.15.11",
        "chai": "^4.3.7",
        "dotenv": "^16.0.3",
        "eslint": "^8.38.0",
        "eslint-config-distube": "^1.6.4",
        "ffmpeg-static": "^5.1.0",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "prettier": "^2.8.7",
        "ts-mocha": "^10.0.0",
        "typedoc": "^0.24.1",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "undici": "^5.21.2"
    },
    "nyc": {
        "extends": "@istanbuljs/nyc-config-typescript",
        "include": [
            "**/!(*.test.*).ts"
        ],
        "exclude": [
            "dist/**/*.*",
            "test/**/*.*"
        ],
        "reporter": [
            "html",
            "lcov",
            "text",
            "text-summary"
        ],
        "report-dir": "coverage"
    }
}
